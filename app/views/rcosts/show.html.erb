<%= render "partials/dashboardlink" %>

	<table class="table table-striped">
		<thead>
			<tr>
			<th style="border: 1px solid black; text-align: center; width: 275px;">Real Cost Entry Date</th>
			<th style="border: 1px solid black; vertical-align: text-top; text-align: center; width: 350px;">Real Cost Entry Title</th>
			<th style="border: 1px solid black; text-align: center; width: 300px;">Real Cost Entry Value</th>
			</tr>
		</thead>
		<tbody>
			<tr style="text-align: center;">
			<td style="border: 1px solid black;"><%= @rCost.date.to_formatted_s(:long) %></td>
			<td style="border: 1px solid black;"><%= @rCost.title %></td>
		 	<td style="border: 1px solid black;">&#36;<%= @rCost.rcost %></td>
		 	<td style="border: 1px solid black; width: 275px;"><%= link_to "Edit Real Cost Entry", edit_user_dashboard_rcost_path(@user, @dashboard, @rCost), method: :get %></td>
		 	<td style="border:none;"></td>
			</tr>
		</tbody>
	</table>
